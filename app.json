{
  "expo": {
    "name": "supervisionsuci",
    "slug": "myapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/imageApp.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E262E"
    },
    "scheme":"com.supervisionsuci.supervisionsuci",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.supervisionsuci.supervisionsuci",
      "supportsTablet": true
    },
    "android": {
      "package": "com.supervisionsuci.supervisionsuci",
      "adaptiveIcon": {
        "foregroundImage": "./assets/imageApp.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode":"pan",
      "permissions": [
        "LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/imageApp.png",
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f2518a2d-12ef-4853-a8da-51712e24ab67"
      }
    }
  }
}
